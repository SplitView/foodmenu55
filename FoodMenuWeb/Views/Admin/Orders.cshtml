@model IEnumerable<FoodMenuWeb.Models.OrderVM>

@{
    ViewBag.Title = "Orders";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Orders</h2>

<div>
    <h4>Order</h4>
    <hr />
    <table class="table table-responsive table-striped">
        <tr>
            <th>Order</th>
            <th>Date</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Item</td>
                <td>@item.Date.Date</td>
            </tr>
        }
    </table>
</div>
